class Solution {
public:
    bool hasAllCodes(string s, int k) {
        unordered_set<string> substr_set;
        int len = s.size();
        int no_of_comb = (1<<k);
        for(int i = 0; i+k <= len && substr_set.size() != no_of_comb; i++){
            substr_set.insert(s.substr(i,k));
        }
        return substr_set.size() == no_of_comb;
    }
};

